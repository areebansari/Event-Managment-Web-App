<% include partials/venues %>
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
	<link rel="stylesheet" href="stylesheets/landing.css">
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-alpha/css/bootstrap.min.css">
	<link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
	<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-alpha/js/bootstrap.min.js"></script>

<div class="jumbotron">
	<h2>Submit A New Venue</h2>
	<a align="center" class="btn btn-primary btn-lg" href="/venues/new">Add New Venue</a>
</div>

<header class = "w3-container w3-teal">
	<h1 align = "center">VENUES</h1>
</header>



<div class = "container"></div>
	<div class="row text-center" style="display:flex; flex-wrap: wrap;">
		<% venues.forEach(function(venues){ %>
			<div class="col-md-3 col-sm-6">
				<div class="thumbnail">
				<img src="<%= venues.image %>">
				<div class="caption">
					<h4><%= venues.name %></h4>
				</div>
				<p>
					<a href="/venues/<%=venues._id %>" class="btn btn-primary">More Info</a>
				</p>
				</div>
			</div>
		<% }); %>
	</div>
</div>

<% include partials/footer %>